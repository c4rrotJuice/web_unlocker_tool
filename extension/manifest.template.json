{
  "manifest_version": 3,
  "name": "__EXTENSION_NAME__",
  "version": "0.1.0",
  "description": "Authenticate with Supabase and check extension unlock usage.",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Web Unlocker"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "host_permissions": [
    "__BACKEND_ORIGIN__/*",
    "__SUPABASE_ORIGIN__/*"
  ]
}

{
  "name": "unlock-heavy-burst",
  "description": "High pressure burst profile focused on unlock endpoints.",
  "virtual_users": 40,
  "ramp_seconds": 30,
  "duration_seconds": 600,
  "request_timeout_seconds": 25,
  "endpoints": [
    {"method": "POST", "path": "/view", "weight": 35, "content_type": "form", "data": {"url": "https://example.com", "unlock": "on"}, "headers": {"Authorization": "Bearer perf-token"}},
    {"method": "POST", "path": "/fetch_and_clean_page", "weight": 45, "content_type": "form", "data": {"url": "https://example.com", "unlock": "on"}, "headers": {"Authorization": "Bearer perf-token"}},
    {"method": "POST", "path": "/api/extension/unlock-permit", "weight": 20, "json": {"target_url": "https://example.com/article", "source": "extension"}, "headers": {"Authorization": "Bearer perf-token"}}
  ]
}
